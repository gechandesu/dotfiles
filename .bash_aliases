# Навигация
alias ls='ls --color=auto'  
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias cls='clear'
alias \.\.='cd ..'
alias du='du -h'


# Запуск программ
alias py='python3'
alias pym='python3 -m' # запустить модуль python как скрипт


# Работа с пакетами
alias destroy='sudo apt remove --purge -y'


